{
  "name": "@coderwyd/eslint-config",
  "type": "module",
  "version": "2.7.1",
  "packageManager": "pnpm@9.9.0",
  "description": "Donny's ESLint config",
  "author": "Donny Wang <donny526@outlook.com> (https://github.com/coderwyd/)",
  "license": "MIT",
  "homepage": "https://github.com/coderwyd/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-vue",
    "eslint-config-react",
    "eslint-config-svelte"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "nr build:typegen && tsup",
    "dev": "npx @eslint/config-inspector",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --flag unstable_ts_config . --fix",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "build:typegen": "tsx scripts/typegen.ts",
    "prepack": "nr build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.5.8",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": "^9.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-svelte": ">=2.35.1",
    "eslint-plugin-tailwindcss": "^3.16.0",
    "svelte-eslint-parser": ">=0.37.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.4.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@stylistic/eslint-plugin": "^2.7.2",
    "@toml-tools/parser": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitest/eslint-plugin": "^1.1.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.5.0",
    "eslint-plugin-command": "^0.2.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-import-x": "^4.2.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^3.4.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "eslint-typegen": "^0.3.1",
    "globals": "^15.9.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.1.0",
    "prettier-plugin-toml": "^2.0.1",
    "prompts": "^2.4.2",
    "vue-eslint-parser": "^9.4.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@eslint-react/eslint-plugin": "^1.13.0",
    "@eslint/config-inspector": "^0.5.4",
    "@stylistic/eslint-plugin-migrate": "^2.7.2",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.4",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "@unocss/eslint-plugin": "^0.62.3",
    "bumpp": "^9.5.2",
    "eslint": "^9.10.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-svelte": "2.43.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "execa": "^9.3.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "jiti": "^1.21.6",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^4.2.19",
    "svelte-eslint-parser": "^0.41.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpx nano-staged"
  },
  "nano-staged": {
    "*": "eslint --flag unstable_ts_config --fix"
  }
}
