{
  "name": "@coderwyd/eslint-config",
  "type": "module",
  "version": "4.1.1",
  "packageManager": "pnpm@10.5.2",
  "description": "Donny's ESLint config",
  "author": "Donny Wang <donny526@outlook.com> (https://github.com/coderwyd/)",
  "license": "MIT",
  "homepage": "https://github.com/coderwyd/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-vue",
    "eslint-config-react",
    "eslint-config-svelte"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "nr build:typegen && tsup",
    "dev": "pnpx @eslint/config-inspector",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:inspector": "pnpm build && pnpx @eslint/config-inspector build",
    "build:typegen": "jiti scripts/typegen.ts",
    "prepack": "nr build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.5.8",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": "^9.5.0",
    "eslint-plugin-react-compiler": "^19.0.0-beta-decd7b8-20250118",
    "eslint-plugin-react-hooks": "^4.6.0 || ^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-svelte": ">=2.35.1",
    "eslint-plugin-tailwindcss": "^3.16.0",
    "svelte-eslint-parser": ">=0.37.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-compiler": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitest/eslint-plugin": "^1.1.37",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.1.0",
    "eslint-plugin-de-morgan": "^1.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-typegen": "^2.0.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "vue-eslint-parser": "^9.4.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/ni": "^23.3.1",
    "@eslint-react/eslint-plugin": "^1.32.0",
    "@eslint/config-inspector": "^1.0.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.10",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "@unocss/eslint-plugin": "^66.1.0-beta.3",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-compiler": "19.0.0-beta-e552027-20250112",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-svelte": "^3.0.3",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "jiti": "^2.4.2",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^5.20.5",
    "svelte-eslint-parser": "^1.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec nano-staged"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
