{
  "name": "@coderwyd/eslint-config",
  "type": "module",
  "version": "3.3.0",
  "packageManager": "pnpm@9.14.4",
  "description": "Donny's ESLint config",
  "author": "Donny Wang <donny526@outlook.com> (https://github.com/coderwyd/)",
  "license": "MIT",
  "homepage": "https://github.com/coderwyd/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-vue",
    "eslint-config-react",
    "eslint-config-svelte"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "nr build:typegen && tsup",
    "dev": "npx @eslint/config-inspector",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --flag unstable_ts_config . --fix",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "build:typegen": "tsx scripts/typegen.ts",
    "prepack": "nr build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.5.8",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": "^9.5.0",
    "eslint-plugin-react-hooks": "^4.6.0 || ^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-svelte": ">=2.35.1",
    "eslint-plugin-tailwindcss": "^3.16.0",
    "svelte-eslint-parser": ">=0.37.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.5.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@toml-tools/parser": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/eslint-plugin": "^1.1.16",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-command": "^0.2.6",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.5.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-jsonc": "^2.18.2",
    "eslint-plugin-n": "^17.14.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-typegen": "^0.3.2",
    "globals": "^15.13.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.1",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.4.2",
    "prompts": "^2.4.2",
    "vue-eslint-parser": "^9.4.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.1",
    "@eslint-react/eslint-plugin": "^1.19.0",
    "@eslint/config-inspector": "^0.6.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "@unocss/eslint-plugin": "^0.65.1",
    "bumpp": "^9.8.1",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "jiti": "^2.4.1",
    "lint-staged": "^15.2.11",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^5.5.4",
    "svelte-eslint-parser": "^0.43.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpx nano-staged"
  },
  "nano-staged": {
    "*": "eslint --flag unstable_ts_config --fix"
  }
}
