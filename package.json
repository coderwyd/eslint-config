{
  "name": "@coderwyd/eslint-config",
  "type": "module",
  "version": "4.3.0",
  "packageManager": "pnpm@10.11.0",
  "description": "Donny's ESLint config",
  "author": "Donny Wang <donny526@outlook.com> (https://github.com/coderwyd/)",
  "license": "MIT",
  "homepage": "https://github.com/coderwyd/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-vue",
    "eslint-config-react",
    "eslint-config-svelte"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "nr build:typegen && tsdown",
    "dev": "pnpx @eslint/config-inspector",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:inspector": "pnpm build && pnpx @eslint/config-inspector build",
    "build:typegen": "jiti scripts/typegen.ts",
    "prepack": "nr build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.5.8",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": "^9.5.0",
    "eslint-plugin-react-compiler": "^19.0.0-beta-decd7b8-20250118",
    "eslint-plugin-react-hooks": "^4.6.0 || ^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-svelte": ">=2.35.1",
    "eslint-plugin-tailwindcss": "^3.16.0",
    "svelte-eslint-parser": ">=0.37.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-compiler": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitest/eslint-plugin": "^1.2.1",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.2.1",
    "eslint-plugin-de-morgan": "^1.2.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.13.3",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.1.0",
    "eslint-plugin-yml": "^1.18.0",
    "eslint-typegen": "^2.2.0",
    "globals": "^16.2.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "vue-eslint-parser": "^10.1.3",
    "yaml-eslint-parser": "^1.3.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@antfu/ni": "^25.0.0",
    "@eslint-react/eslint-plugin": "^1.50.0",
    "@eslint/config-inspector": "^1.0.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.15.29",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "@unocss/eslint-plugin": "^66.1.3",
    "bumpp": "^10.1.1",
    "eslint": "^9.28.0",
    "eslint-plugin-react-compiler": "19.0.0-beta-ebf51a3-20250411",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-svelte": "^3.9.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "jiti": "^2.4.2",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.13.0",
    "svelte": "^5.33.14",
    "svelte-eslint-parser": "^1.2.0",
    "tsdown": "0.12.6",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec nano-staged"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
