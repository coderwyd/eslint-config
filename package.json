{
  "name": "@coderwyd/eslint-config",
  "type": "module",
  "version": "1.1.0-beta.3",
  "packageManager": "pnpm@8.10.5",
  "author": "Donny Wang <donny526@outlook.com> (https://github.com/coderwyd/)",
  "license": "MIT",
  "homepage": "https://github.com/coderwyd/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm",
    "lint": "pnpm run stub && eslint .",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "astro-eslint-parser": "^0.16.0",
    "eslint-config-flat-gitignore": "^0.1.1",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-antfu": "1.0.9",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.9",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-yml": "^1.10.0",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.3.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-migrate": "^0.0.6",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "eslint-flat-config-viewer": "^0.1.3",
    "eslint-plugin-sort-keys": "^2.3.5",
    "nano-staged": "^0.8.0",
    "react": "^18.2.0",
    "simple-git-hooks": "^2.9.0",
    "sucrase": "^3.34.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpx nano-staged"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
